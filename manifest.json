{
    "manifest_version": 2,
    "name": "Price Safeguard",
    "description": "A stock price notification for personal use",
    "version": "0.1",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "settingStock.html"
    },
    "permissions": [
